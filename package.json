{
  "devDependencies": {
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.0.0",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "gh-pages": "^6.3.0",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.3",
    "mini-css-extract-plugin": "^2.9.2",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^11.0.0",
    "postcss-csso": "^6.0.1",
    "terser-webpack-plugin": "^5.3.11",
    "webpack": "^5.83.0",
    "webpack-cli": "^5.0.0"
  },
  "homepage": "https://Hgnim.github.io/MJYY_Web",
  "scripts": {
    "github-action_build": "npm-run-all build:runWebpack build:css-animation_linuxSh",
    "build": "npm-run-all build:runWebpack build:css-animation_wincmd",
    "build:runWebpack": "webpack",
    "build:css-animation_wincmd": "for %f in (.\\src\\css\\animation\\*.css) do npx postcss \"%f\" -o \".\\dist\\css\\animation\\%~nxf\" --verbose",
    "build:css-animation_linuxSh":"find ./src/css/animation -name '*.css' -exec sh -c 'npx postcss \"$0\" -o \"./dist/css/animation/$(basename \"$0\")\" --verbose' {} \\;",

    "deploy": "gh-pages -d dist"
  }
}
